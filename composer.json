{
    "name": "proklung/bitrix.module.boilerplate",
    "description": "PHP boilerplate for Bitrix modules",
    "type": "bitrix",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/ProklUng/bitrix.module.boilerplate/issues",
        "source": "https://github.com/ProklUng/bitrix.module.boilerplate"
    },
    "authors": [
        {
            "name": "Gavrilov Fedy",
            "email": "fedor.gavrilov.75@mail.ru"
        }
    ],
    "autoload": {
        "psr-4": {"ProklUng\\Module\\Boilerplate\\": "src"}
    },
    "require": {
        "php": ">=7.3"
    },
    "require-dev": {
        "proklung/bitrix-phpunit-testing-tools": "^1.0"
    },
    "extra": {
        "installer-paths": {
            "vendor/sheerockoff/bitrix-ci/files/bitrix/modules/{$name}/": ["type:bitrix-module"]
        }
    }
}
